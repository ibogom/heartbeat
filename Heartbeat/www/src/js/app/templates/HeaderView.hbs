<div>
    <div class="left-el icons">
        {{#if showSettings}}
            <span class="settings"></span>
        {{/if}}
        {{#if showMenu}}
            <span class="menu"></span>
        {{/if}}
        {{#if showLeftBack}}
            <span class="left-back"></span>
        {{/if}}
    </div>
    <div class="middle-el">
        {{#if showTitle}}
            <h1 class="title">{{Title}}</h1>
        {{/if}}
        {{#if showlist}}
            <ul class="list-switch">
                {{#if showHomeNav}}
                    <li class="player active">Beats</li>
                    <li class="rating">Rating</li>
                {{/if}}
                {{#if showPlaylistNav}}
                    <li class="music active">Music</li>
                    <li class="friends">Friends</li>
                {{/if}}
            </ul>
        {{/if}}
    </div>
    <div class="right-el icons">
        {{#if showRightBack}}
            <span class="right-back"></span>
        {{/if}}
        {{#if showPlaylist }}
            <span class="playlist"></span>
        {{/if}}
        {{#if showSearch }}
            <span class="search"></span>
            <span class="cancel">Cancel</span>
        {{/if}}
    </div>
</div>